<form class="regForm" id="registerFrom">
  <p class="regLable">이름</p>
  <input
    class="input-group-text"
    type="text"
    id="name"
    name="name"
    placeholder="이름을 입력해주세요."
    value="example"
    required
  />
  <br />
  <p class="regLable">email</p>
  <input
    class="input-group-text"
    type="email"
    id="regEmail"
    name="email"
    placeholder="이메일을 입력해주세요."
    value="example@example.com"
    required
  />
  <br />
  <p class="regLable">비밀번호</p>
  <input
    class="input-group-text"
    type="password"
    id="regPassword"
    name="password"
    value="123123"
    required
  />
  <br />
  <p class="regLable">비밀번호 확인</p>
  <input
    class="input-group-text"
    type="password"
    id="passwordConfirm"
    name="passwordConfirm"
    value="123123"
    required
  />
  <br />
  <button
    class="btn btn-secondary btn-sm input-group-text regSubmit"
    type="submit"
    id="regSubmitBtn"
  >
    회원가입
  </button>
</form>
